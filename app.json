{
  "expo": {
    "name": "LanguageAppAI",
    "slug": "LanguageAppAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.uygarerenn.LanguageAppAI"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
        "expo-speech-recognition"
    ]
  }
}
